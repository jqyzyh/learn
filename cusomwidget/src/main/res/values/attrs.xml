<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundRectImageView">
        <attr name="imageRadius" format="dimension" />
        <attr name="roundType" format="enum">
            <enum name="all" value="1" />
            <enum name="left" value="2" />
            <enum name="rigth" value="3" />
            <enum name="top" value="4" />
            <enum name="bottom" value="5" />
            <enum name="circle" value="6"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="WrapLayout">
        <attr name="gravity" format="enum">
            <enum name="left" value="3" />
            <enum name="center_horizontal" value="1" />
            <enum name="rigth" value="5" />
        </attr>
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />

    </declare-styleable>

    <declare-styleable name="QQDrawerLayout">
        <attr name="DrawerType" format="enum">
            <enum name="content" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>
    
    <declare-styleable name="ColorIndicator">
        <attr name="IndicatorColor" format="color"/>
        <attr name="NormalColor" format="color"/>
        <attr name="IndicatorTextSize" format="dimension"/>
        <attr name="NormalTextSize" format="dimension"/>
        <attr name="TabSpace" format="dimension"/>
        <attr name="IndicatorTypeface" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
        </attr>
        <attr name="NormalTypeface" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
        </attr>
    </declare-styleable>
</resources>